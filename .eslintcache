mport { getBookedTrip, payOrder, savePaymentMethod } from '../../../actions/bookingActions';\r\nimport { ORDER_PAY_RESET } from '../../../constants/bookingConstants';\r\n\r\n\r\nconst SelectPaymentBox = ({ bookingId }) => {\r\n\r\n    const [sdkReady, setSdkReady] = useState(false)\r\n\r\n    const dispatch = useDispatch()\r\n\r\n    const bookedTrip = useSelector(state => state.bookedTrip)\r\n    const { bookedTrip: booking } = bookedTrip\r\n\r\n    const paymentMethod = useSelector(state => state.paymentMethod.paymentMethod)\r\n\r\n    const orderPay = useSelector((state) => state.orderPay)\r\n    const { success, loading } = orderPay\r\n\r\n\r\n    const setPaymentToPaypal = () => {\r\n        dispatch(savePaymentMethod('paypal'))\r\n    }\r\n    const setPaymentToStripe = () => {\r\n        dispatch(savePaymentMethod('stripe'))\r\n    }\r\n\r\n    useEffect(() => {\r\n        const addPayPalScript = async () => {\r\n            const { data: clientId } = await axios.get('/api/config/paypal')\r\n            const script = document.createElement('script')\r\n            script.type = 'text/javascript'\r\n            script.src = `https://www.paypal.com/sdk/js?client-id=${clientId}`\r\n            script.async = true\r\n            script.onload = () => {\r\n                setSdkReady(true)\r\n            }\r\n            document.body.appendChild(script)\r\n        }\r\n\r\n\r\n        if (!booking || success) {\r\n            dispatch({ type: ORDER_PAY_RESET })\r\n            dispatch(getBookedTrip(bookingId))\r\n        } else if (!booking.isPaid) {\r\n            if (!window.paypal) {\r\n                addPayPalScript()\r\n            } else {\r\n                setSdkReady(true)\r\n            }\r\n        }\r\n    }, [dispatch, bookingId, success, booking])\r\n\r\n    const successPaymentHandler = (paymentResult) => {\r\n        console.log(paymentResult)\r\n        dispatch(payOrder(bookingId, paymentResult, { paymentMethod: paymentMethod }))\r\n\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"payment-box-wrap ml-3 pb-5\">\r\n            <div className=\"row\">\r\n                <h5 className=\"mb-3\">Payment procedure</h5>\r\n            </div>\r\n            {!booking.isPaid ? (\r\n                <div className=\"row\">\r\n                    <div className=\"col-lg-4\">\r\n                        <button className='paypal-btn' onClick={setPaymentToPaypal} style={{ backgroundImage: `url(\"https://p.kindpng.com/picc/s/12-123352_dawn-ellmore-employment-paypal-logo-png-transparent-png.png\")`, backgroundSize: 'cover', backgroundPosition: 'center', }}></button>\r\n                    </div>\r\n                    <div className=\"col-lg-4\">\r\n                        <button className='stripe-btn' onClick={setPaymentToStripe} style={{ backgroundImage: `url(\"https://upload.wikimedia.org/wikipedia/commons/thumb/b/ba/Stripe_Logo%2C_revised_2016.svg/1280px-Stripe_Logo%2C_revised_2016.svg.png\")`, backgroundSize: 'cover', backgroundPosition: 'center', }}></button>\r\n                    </div>\r\n                </div>\r\n            ) : null}\r\n            <div className=\"row mt-3 d-flex justify-content-start\">\r\n                <div className=\"payment-details-box d-flex justify-content-center\">\r\n                    {paymentMethod === 'paypal' && !booking.isPaid ? (\r\n                        <div className=\"justify-content-center d-flex mt-4\">\r\n                            <PayPalButton amount={booking.totalPrice} onSuccess={successPaymentHandler} />\r\n                        </div>\r\n                    ) :\r\n                        paymentMethod === 'stripe' && !booking.isPaid ? (\r\n                            <p>Stripe Buttons</p>\r\n                        ) : !booking.isPaid ? (\r\n                            <h6 className=\"mt-3\">Please select a payment method to proceed payment</h6>\r\n                        ) : (\r\n                            <h5 className=\"mt-3\" style={{ color: 'green' }}>Your payment is complete</h5>\r\n                        )}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default SelectPaymentBox;\r\n","C:\\Users\\hp\\travelholic\\src\\actions\\tripActions.js",[],"C:\\Users\\hp\\travelholic\\src\\actions\\userActions.js",[],"C:\\Users\\hp\\travelholic\\src\\actions\\bookingActions.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\book-a-trip\\book-a-trip-components\\TripRating.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\book-a-trip\\book-a-trip-components\\Day.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\AllQuestions.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\TopicQuestions.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\TopicQuestionCard.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\DetailQuestionCard.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\MostViewedQuestions.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\FeaturedTopics.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\MostViewedQuestionCard.js",[],"C:\\Users\\hp\\travelholic\\src\\axios.js",[],"C:\\Users\\hp\\travelholic\\src\\components\\forum\\ProfleQuestionCard.js",[],{"ruleId":"335","replacedBy":"336"},{"ruleId":"337","replacedBy":"338"},{"ruleId":"339","severity":1,"message":"340","line":4,"column":8,"nodeType":"341","messageId":"342","endLine":4,"endColumn":13},{"ruleId":"339","severity":1,"message":"343","line":10,"column":8,"nodeType":"341","messageId":"342","endLine":10,"endColumn":21},{"ruleId":"339","severity":1,"message":"344","line":67,"column":8,"nodeType":"341","messageId":"342","endLine":67,"endColumn":24},{"ruleId":"339","severity":1,"message":"345","line":7,"column":8,"nodeType":"341","messageId":"342","endLine":7,"endColumn":18},{"ruleId":"346","severity":1,"message":"347","line":32,"column":7,"nodeType":"348","endLine":32,"endColumn":9,"suggestions":"349"},{"ruleId":"339","severity":1,"